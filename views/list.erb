<h2><%= @type_name %>一覧</h2>
<table id="list" class="table">
  <tr><th>名前</th></tr>
</table>

<script>
  $(function () {
      $.getJSON("/get/list?type=<%= @type %>", function(list){
        for (var i in list) {
          <% if @type == "book" %>
          $("#list").append("<tr><td>" + list[i].title + "</td></tr>")
          <% else %>
          $("#list").append("<tr><td>" + list[i].name + "</td></tr>")
          <% end %>
        }
      });
    });
</script>